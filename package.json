{
    "name": "mmpula",
    "version": "0.2.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "lint": "eslint src --ext .vue,.js --fix",
        "format": "prettier --write \"src/**/*.{vue,js,json,css}\" \"*.{json,cjs,md}\"",
        "format:check": "prettier --check \"src/**/*.{vue,js,json,css}\" \"*.{json,cjs,md}\"",
        "prepare": "husky"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^7.1.0",
        "codemirror": "^5.65.0",
        "codemirror-editor-vue3": "^2.8.0",
        "love-is-love-spinners": "^1.0.0",
        "mdb-vue-ui-kit": "^7.0.0",
        "vue": "^3.4.0",
        "vue-good-table-next": "^0.2.1",
        "vue-gtag": "^2.0.1",
        "vue-router": "^4.3.0"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.1.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-vue": "^9.24.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.7",
        "prettier": "^3.2.0",
        "vite": "^5.4.0"
    },
    "homepage": "https://github.com/melissapula/melissapula.github.io",
    "lint-staged": {
        "src/**/*.{vue,js}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,css,md,cjs}": [
            "prettier --write"
        ]
    }
}
